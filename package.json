{
  "name": "react-boxplot",
  "version": "0.4.0",
  "description": "Simple SVG box plots in React",
  "main": "dist/boxplot.js",
  "repository": "https://www.github.com/paulmelnikow/react-boxplot",
  "homepage": "http://github.com/paulmelnikow/react-boxplot",
  "bugs": "https://www.github.com/paulmelnikow/react-boxplot/issues",
  "author": "Paul Melnikow <github@paulmelnikow.com>",
  "license": "BSD-2-Clause",
  "keywords": [
    "react",
    "chart",
    "boxplot",
    "svg"
  ],
  "scripts": {
    "build": "jsx --extension jsx src/ dist/",
    "watch": "jsx --extension jsx --watch src/ dist/",
    "start": "http-server -p 8086",
    "open": "((sleep 1 && opn 'http://localhost:8086/examples/') &) && http-server -p 8086",
    "reopen": "opn 'http://localhost:8086/'"
  },
  "devDependencies": {
    "http-server": "~0.8.0",
    "opn": "~1.0.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-tools": "~0.13.2"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "create-react-class": "^15.6.2",
    "prop-types": "^15.6.0",
    "simple-statistics": "^1.0.0",
    "underscore": "~1.8"
  }
}
